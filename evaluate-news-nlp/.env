var aylien = require('aylien_textapi');

// Configure the aylien API client
var textapi = new aylien({
   application_id: process.env.API_ID, // Access API ID from .env
   application_key: process.env.API_KEY // Access API Key from .env
});

// Just to check, you can log the API key to make sure it's working (for debugging purposes):
console.log(`Your API key is ${process.env.API_KEY}`);
